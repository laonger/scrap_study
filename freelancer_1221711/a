                        <button data-toggle="dropdown" class="btn btn-default dropdown-toggle" data-val="2" type="button" id="DividendoTipoCalculacion">
                            <span class="current-value">UF</span>&nbsp;<i class="fa fa-angle-down"></i>
                        </button>
                        <ul id="tipo-moneda" class="dropdown-menu dropdown-menu-mini js-autochange">
                        </ul>
                    </div>
                    <div>
                        <span class="help-inline"></span>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-md-6 tablet-wide">
            <div class="form-group" style="margin-bottom: 0">
                <p style="margin-bottom: 4px"><strong>Tasa de inter&#233;s plazo</strong></p>
                <input name="DividendoTasa" type="text" class="form-control" id="DividendoTasa" placeholder="%" value="5,8" />&nbsp;%&nbsp;
                <div class="btn-group" id="ddnAnios">
                    <button data-toggle="dropdown" id="DividendoPeriodo" class="btn btn-default dropdown-toggle" data-val="20" type="button">
                        <span class="current-value">20</span>&nbsp;
                        <i class="fa fa-angle-down"></i>
                    </button>
                    <ul class="dropdown-menu dropdown-menu-mini js-autochange">
                        <li><a data-val="5" href="#">&nbsp;5</a></li>
                        <li><a data-val="8" href="#">&nbsp;8</a></li>
                        <li><a data-val="12" href="#">12</a></li>
